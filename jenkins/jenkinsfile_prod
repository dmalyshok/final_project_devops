pipeline {
    agent {label 'main'}

    stages {
        stage('1-Deploy_From_S3_To_Prod') {
            steps {
                withAWS(region:'eu-central-1',credentials:'aws-cred') {
                  sh 'echo "Download content with AWS creds"'
                     s3Download(file:"/home/jenkins/apache_artifact_v58", bucket:"dmitry.malyshok.org.ua", path:"apache_artifact_v58", force:true)
                  sh 'ls -la"'
                }
           }  
        }
    }
}