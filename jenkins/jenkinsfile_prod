pipeline {
    agent {label 'prod'}

    stages {
        stage('1-Deploy_From_S3_To_Prod') {
            steps {
                withAWS(region:'eu-central-1',credentials:'aws-cred') {
                  sh 'echo "Download content with AWS creds"'
                     s3Download(file:"apache_artifact_v8", bucket:"dmitry.malyshok.org.ua", path:"apache_artifact_v8", force:true)
                  sh 'sudo mv apache_artifact_v8 /home/artifact/'
                }
           }  
        }
    }
}